<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Kuali Coeus Reporting - Jasper Reports</display-name>
  <listener>
    <listener-class>
      org.springframework.web.context.ContextLoaderListener
    </listener-class>
  </listener>
  <servlet>
    <servlet-name>kc-jasperreports</servlet-name>
    <servlet-class>com.rsmart.kuali.coeus.reporting.jasperreports.ReportingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>kc-jasperreports</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
